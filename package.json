{
  "name": "meal-planner",
  "version": "1.0.0",
  "scripts": {
    "deploy-api": "sls api:deploy",
    "deploy-web-client": "sls web-client:deploy",
    "build-web-client": "webpack --config ./packages/web-client/webpack.config.prod.babel.js --progress",
    "upload-web-client": "aws s3 sync ./packages/web-client/.dist s3://meal-planner-web-client",
    "serve-web-client-dev": "webpack serve --config ./packages/web-client/webpack.config.dev.babel.js",
    "deploy-shared-infra": "sls shared-infra:deploy"
  },
  "private": true,
  "workspaces": [
    "packages/api",
    "packages/web-client"
  ],
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-proposal-decorators": "7.23.7",
    "@babel/plugin-proposal-do-expressions": "7.23.3",
    "@babel/plugin-proposal-export-default-from": "7.23.3",
    "@babel/plugin-proposal-function-bind": "7.23.3",
    "@babel/plugin-proposal-function-sent": "7.23.3",
    "@babel/plugin-proposal-pipeline-operator": "7.23.3",
    "@babel/plugin-proposal-throw-expressions": "7.23.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-class-properties": "7.23.3",
    "@babel/plugin-transform-export-namespace-from": "7.23.4",
    "@babel/plugin-transform-json-strings": "7.23.4",
    "@babel/plugin-transform-logical-assignment-operators": "7.23.4",
    "@babel/plugin-transform-nullish-coalescing-operator": "7.23.4",
    "@babel/plugin-transform-numeric-separator": "7.23.4",
    "@babel/plugin-transform-optional-chaining": "7.23.4",
    "@babel/plugin-transform-react-constant-elements": "7.23.3",
    "@babel/plugin-transform-react-inline-elements": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.7",
    "@babel/preset-react": "7.23.3",
    "@babel/register": "7.23.7",
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@serverless/compose": "1.3.0",
    "babel-loader": "9.1.3",
    "babel-plugin-dev-expression": "0.2.3",
    "babel-plugin-transform-imports": "2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "prettier": "3.1.1"
  }
}
